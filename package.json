{
  "name": "hono-client-mini",
  "version": "1.0.2",
  "description": "hono/client for mini program",
  "keywords": [
    "hono",
    "mini program",
    "wx-request"
  ],
  "github": "https://github.com/horizon0514/hono-client-mini",
  "author": "horizon0514",
  "homepage": "https://github.com/horizon0514/hono-client-mini",
  "repository": {
    "type": "git",
    "url": "https://github.com/horizon0514/hono-client-mini.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "tsup src/index.ts --format cjs,esm --dts-resolve",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "dependencies": {
    "@zlyboy/wx-formdata": "^1.0.2"
  },
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "miniprogram-api-typings": "^4.1.0",
    "prettier": "2.8.4",
    "tsup": "6.6.3",
    "typescript": "4.9.5",
    "vitest": "0.28.5"
  },
  "peerDependencies": {
    "hono": "^4.0.0"
  }
}
